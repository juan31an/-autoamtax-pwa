{
  "name": "autoamtax-pwa",
  "version": "2.0.0",
  "type": "module",
  "description": "AutoamTax - Accessible Transportation Management PWA",
  "keywords": ["pwa", "accessibility", "transportation", "vue3", "taxi"],
  "author": "AutoamTax Partners",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "android": "npm run build && npx cap sync android && npx cap run android",
    "android:dev": "npm run build && npx cap sync android && npx cap run android --livereload --external",
    "ios": "npm run build && npx cap sync ios && npx cap run ios",
    "sync": "npx cap sync",
    "capacitor:add": "npx cap add",
    "serve:dist": "vite preview --host 0.0.0.0",
    "analyze": "vite-bundle-analyzer",
    "lighthouse": "lhci autorun"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "@vueuse/core": "^10.7.2",
    "@headlessui/vue": "^1.7.17",
    "@heroicons/vue": "^2.0.18",
    "firebase": "^10.12.2",
    "leaflet": "^1.9.4",
    "chart.js": "^4.4.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^3.3.1",
    "@capacitor/core": "^5.7.0",
    "@capacitor/app": "^5.0.6",
    "@capacitor/camera": "^5.0.7",
    "@capacitor/geolocation": "^5.0.6",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/local-notifications": "^5.0.6",
    "@capacitor/network": "^5.0.6",
    "@capacitor/preferences": "^5.0.6",
    "@capacitor/push-notifications": "^5.1.0",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@capacitor/toast": "^5.0.6"
  },
  "devDependencies": {
    "@capacitor/android": "^5.7.0",
    "@capacitor/cli": "^5.7.0",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "^24.0.0",
    "@lhci/cli": "^0.12.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27",
    "workbox-window": "^7.0.0",
    "@playwright/test": "^1.41.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}